<?xml version="1.0" encoding="UTF-8"?>
<Dataset name="joinNew_simple.ncml"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://xml.opendap.org/ns/DAP2"
xsi:schemaLocation="http://xml.opendap.org/ns/DAP2  http://xml.opendap.org/dap/dap2.xsd">


    <Array name="V">
        <Attribute name="units" type="String">
            <value>bohms</value>
        </Attribute>
        <Int32/>
        <dimension name="day" size="2"/>
        <dimension name="station" size="5"/>
    </Array>
    <Array name="station">
        <Attribute name="Desc" type="String">
            <value>I am in the first dataset, so WILL be in the output!</value>
        </Attribute>
        <String/>
        <dimension name="station" size="5"/>
    </Array>
    <Array name="W">
        <Attribute name="Desc" type="String">
            <value>An extra variables that will be merged into the output.</value>
        </Attribute>
        <Int32/>
        <dimension name="station" size="5"/>
    </Array>
    <Array name="V_expected">
        <Int32/>
        <dimension name="day" size="2"/>
        <dimension name="station" size="5"/>
    </Array>

    <dataBLOB href=""/>
</Dataset>
